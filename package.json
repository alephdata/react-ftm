{
  "name": "@alephdata/vislib",
  "version": "1.8.4",
  "description": "Graph network visualisation tool for aleph/followthemoney ",
  "keywords": [],
  "files": [
    "dist/**/*"
  ],
  "author": "OCCRP Data <data@occrp.org>",
  "main": "dist/vislib.umd.js",
  "module": "dist/vislib.es5.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/alephdata/vislib"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "translate": "npm run messages && npm run json2pot && npm run po2json",
    "messages": "formatjs extract --out-file i18n/messages.json 'src/**/*.{ts,tsx}'",
    "json2pot": "react-intl-po json2pot 'i18n/messages.json' -o 'i18n/messages.pot'",
    "po2json": "react-intl-po po2json 'i18n/translations/*.po' -m 'i18n/messages.json' -o src/translations/translations.json",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rm -f dist/vislib.*",
    "build": "rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@alephdata/followthemoney": "^1.31.4",
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/select": "^3.13.0",
    "@blueprintjs/table": "^3.8.6",
    "@formatjs/cli": "^1.6.14",
    "@rollup/plugin-json": "^4.0.3",
    "@types/bezier-js": "0.0.8",
    "@types/classnames": "^2.2.10",
    "@types/d3": "^5.7.2",
    "@types/jest": "^25.1.4",
    "@types/node": "^14.0.4",
    "@types/node-sass": "^4.11.0",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.5",
    "bezier-js": "^2.5.1",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.11",
    "hoist-non-react-statics": "^3.3.2",
    "jest": "^25.2.2",
    "jest-config": "^26.0.1",
    "node-sass": "^4.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.11.0",
    "react-intl-po": "^2.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "tslib": "^2.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/select": "^3.12.3",
    "@blueprintjs/table": "^3.8.3",
    "react": "^16.13.1",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "d3-force": "^2.0.1",
    "dagre": "^0.8.4",
    "filesize": "^6.1.0",
    "git": "^0.1.5",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react-datasheet": "^1.4.1",
    "react-draggable": "^3.3.0",
    "react-intl": "^3.12.0",
    "save-dev": "0.0.1-security",
    "truncate": "^2.1.0"
  }
}
