{
  "name": "@alephdata/react-ftm",
  "version": "2.3.15",
  "description": "React UI component library for aleph/followthemoney",
  "keywords": [],
  "files": [
    "dist/**/*"
  ],
  "author": "OCCRP Data <data@occrp.org>",
  "main": "dist/react-ftm.js",
  "module": "dist/react-ftm.es5.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alephdata/react-ftm.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "messages": "formatjs extract --out-file i18n/messages.json 'src/**/*.{ts,tsx}'",
    "concat-translations": "node i18n/concatTranslations.js",
    "compile-translations": "formatjs compile-folder i18n/translations i18n/translations",
    "lint": "eslint src",
    "prebuild": "rm -f dist/react-ftm.*",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack serve --config webpack.dev.js --port 8081 --mode development --open",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^3.1.5",
    "@types/bezier-js": "^0.0.8",
    "@types/classnames": "^2.2.11",
    "@types/d3": "^6.3.0",
    "@types/dagre": "^0.7.44",
    "@types/google-libphonenumber": "^7.4.19",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/node-sass": "^4.11.0",
    "@types/numeral": "^2.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/recharts": "^1.8.18",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.22.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.11",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.1.0",
    "eslint": "^7.21.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "git": "^0.1.5",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.1",
    "json-loader": "^0.5.7",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.2",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "@alephdata/followthemoney": "^2.0.0",
    "@blueprintjs/core": "^3.0.0",
    "@blueprintjs/select": "^3.0.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.21",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^5.0.0"
  },
  "dependencies": {
    "bezier-js": "^2.1.6",
    "d3-force": "^2.1.1",
    "d3-scale": "^3.2.3",
    "dagre": "^0.8.4",
    "filesize": "^6.1.0",
    "google-libphonenumber": "^3.2.19",
    "numeral": "^2.0.6",
    "react-countup": "^4.3.3",
    "react-datasheet": "^1.4.9",
    "react-draggable": "^4.4.3",
    "recharts": "^2.0.9",
    "truncate": "^2.1.0"
  },
  "bugs": {
    "url": "https://github.com/alephdata/react-ftm/issues"
  },
  "homepage": "https://github.com/alephdata/react-ftm#readme",
  "directories": {
    "test": "test"
  }
}
