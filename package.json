{
  "name": "@alephdata/vislib",
  "version": "1.2.0",
  "description": "Graph network visualisation tool for aleph/followthemoney ",
  "keywords": [],
  "files": [
    "dist/**/*"
  ],
  "author": "OCCRP Data <data@occrp.org>",
  "main": "dist/vislib.umd.js",
  "module": "dist/vislib.es5.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/alephdata/vislib"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rm -f dist/vislib.*",
    "build": "rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@alephdata/followthemoney": "^1.21.5",
    "@blueprintjs/core": "^3.15.1",
    "@blueprintjs/datetime": "^3.14.0",
    "@blueprintjs/select": "^3.11.1",
    "@blueprintjs/table": "^3.8.1",
    "@types/bezier-js": "0.0.8",
    "@types/classnames": "^2.2.7",
    "@types/d3": "^5.7.2",
    "@types/jest": "^24.0.21",
    "@types/node": "^10.14.4",
    "@types/node-sass": "^4.11.0",
    "@types/numeral": "^0.0.26",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "bezier-js": "^2.4.4",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.3",
    "jest": "^24.5.0",
    "jest-config": "^24.7.1",
    "node-sass": "^4.12.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.26.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "@alephdata/followthemoney": "^1.21.3",
    "@blueprintjs/core": "^3.15.1",
    "@blueprintjs/datetime": "^3.9.0",
    "@blueprintjs/select": "^3.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "d3-force": "^2.0.1",
    "dagre": "^0.8.4",
    "numeral": "^2.0.6",
    "react-draggable": "^3.3.0",
    "save-dev": "0.0.1-security",
    "truncate": "^2.0.1"
  }
}
