{
  "name": "@alephdata/react-ftm",
  "version": "1.11.0-alpha",
  "description": "React UI component library for aleph/followthemoney",
  "keywords": [],
  "files": [
    "dist/**/*"
  ],
  "author": "OCCRP Data <data@occrp.org>",
  "main": "dist/react-ftm.js",
  "module": "dist/react-ftm.es5.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alephdata/react-ftm.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "messages": "formatjs extract --out-file i18n/messages.json 'src/**/*.{ts,tsx}'",
    "concat-translations": "node i18n/concatTranslations.js",
    "compile-translations": "formatjs compile-folder i18n/translations i18n/translations",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rm -f dist/react-ftm.*",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@alephdata/followthemoney": "2.1.8",
    "@blueprintjs/core": "^3.30.1",
    "@blueprintjs/select": "^3.13.6",
    "@formatjs/cli": "^2.8.0",
    "@types/bezier-js": "0.0.8",
    "@types/classnames": "^2.2.10",
    "@types/d3": "^5.7.2",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.2",
    "@types/node-sass": "^4.11.0",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.5",
    "@types/recharts": "^1.8.13",
    "bezier-js": "^2.5.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.11",
    "css-loader": "^4.3.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^25.2.2",
    "jest-config": "^26.4.2",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.11.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/select": "^3.12.3",
    "react": "^16.13.1",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "d3-force": "^2.1.1",
    "d3-scale": "^3.2.2",
    "dagre": "^0.8.4",
    "filesize": "^6.1.0",
    "git": "^0.1.5",
    "moment": "^2.27.0",
    "numeral": "^2.0.6",
    "react-datasheet": "^1.4.8",
    "react-draggable": "^4.4.3",
    "react-intl": "^5.8.4",
    "recharts": "^1.8.5",
    "save-dev": "0.0.1-security",
    "truncate": "^2.1.0"
  },
  "bugs": {
    "url": "https://github.com/alephdata/react-ftm/issues"
  },
  "homepage": "https://github.com/alephdata/react-ftm#readme",
  "directories": {
    "test": "test"
  }
}
